            },
            "Redactar-NombreRol": {
              "type": "Compose",
              "inputs": "@outputs('ObtenerUnaFilaPorId-TablaEquipos')?['body']['teamroles_association']",
              "runAfter": {
                "ObtenerUnaFilaPorId-TablaEquipos": [
                  "Succeeded"
                ]
              }
            }
          },
          "runAfter": {
            "Ambito-CrearEquipoTecnicoCampania": [
              "Succeeded"
            ]
