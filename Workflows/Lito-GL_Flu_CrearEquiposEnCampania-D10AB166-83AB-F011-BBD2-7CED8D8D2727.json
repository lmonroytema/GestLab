        "Ambito-EquipoResponsableCampania": {
          "type": "Scope",
          "actions": {
            "Redactar-GUIDEquipoCreado": {
              "type": "Compose",
              "inputs": "@{outputs('AgregarUnaFilaNueva-TablaEquipo')?['body/teamid']}\n"
